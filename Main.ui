<layout alpha="1" backgroundColor="#FFFFFFFF" fontFamily="Roboto, sans-serif" paddingRight="18" paddingTop="18" paddingBottom="18" paddingLeft="18" name="Main">
    <datasource type="Main" name="main"/>
    <navigationBar title="Wash Your Hands" hideBackButton="false">
        <leftButtons/>
        <rightButtons/>
    </navigationBar>
    <label wpercentage="1" alpha="1" marginTop="0" marginBottom="8" marginLeft="0" marginRight="0" paddingRight="2" paddingTop="2" paddingBottom="2" paddingLeft="2" textColor="#135F8DFF" fontFamily="Roboto-Bold" fontSize="80" textAlignment="center" bind="main.time"/>
    <image marginBottom="40" marginTop="20" width="350" height="350" selfAlignment="center" bind="main.image"/>
    <button height="100" textAlignment="center" cornerRadius="1000" alpha="1" backgroundColor="#12AF3EFF" fontSize="30" marginTop="4" marginBottom="8" paddingRight="8" paddingTop="8" paddingBottom="8" paddingLeft="8" textColor="#ffffff" width="100" selfAlignment="center" fontFamily="Roboto-Bold" text="Start">
        <selectedState/>
        <highlightedState/>
        <event on="TapEvent">
            <action type="RunRuleAction" ruleContext="MainContext" ruleName="start"/>
        </event>
    </button>
</layout>